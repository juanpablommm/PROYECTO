<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:layout_margin="40dp"
    android:background="@drawable/cambia"
    >



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"/>

    <TextView
        android:id="@+id/textViewnombreConsultar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Nombre"
        android:textSize="17dp"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textViapellidoConsultar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Apellido"
        android:textSize="17dp"
        />

    <TextView
        android:id="@+id/textViatelefonoConsultar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Telefono"
        android:textSize="17dp"
        />

    <TextView
        android:id="@+id/textVicedulaConsultar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Cedula"
        android:textSize="17dp"
        />
    <TextView
        android:id="@+id/textCorreoConsultar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Correo"
        android:textSize="17dp"
        />

    <TextView
        android:id="@+id/textVicontraseñaConsultar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Contraseña"
        android:textSize="17dp"
        />
    <TextView
        android:id="@+id/textVirolConsultar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Rol"
        android:textSize="17dp"
        />

    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="20dp"/>






</LinearLayout>